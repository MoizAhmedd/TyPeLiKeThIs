
{
	"name": "tYpElIkEtHis",
	"version": "1.0.1",
	"description": "tYpE lIkE tHis on any website",
  "manifest_version": 2,
	"permissions": [
	"<all_urls>",
	"tabs",
	"storage",
	"clipboardRead",
	"clipboardWrite",
	"activeTab"
	],
	"content_scripts": [
	  {
		"matches": [
		  "<all_urls>"
		],
		"js": [
		  "content.js"
		],
		"match_about_blank":true,
		"all_frames":true
	  }
	],
	"background": {
		"scripts": [
	   "background.js"
	]
	},
	"browser_action": {
	"default_title": "TyPeLiKeThIs"
	}
  }